      "cell_type": "code",
      "metadata": {
        "id": "Y-rRPhpDEY93"
      },
      "source": [
        "def prepare(a,b):\n",
        "  temp = []\n",
        "  t = np.shape(a)[0]\n",
        "  for i in range(t):\n",
        "    temp.append(list(zip(a[i],b[i])))\n",
        "  return np.array(temp)\n"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "r9E0Ao4VEY93"
      },
      "source": [
        "traindata = prepare(timetr,atr)"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "7WlzL8f0EY93"
      },
      "source": [
        "testdata = prepare(timetest,atest)"
      ],
      "execution_count": null,
      "outputs": []
