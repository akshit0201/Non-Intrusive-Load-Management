      "source": [
        "test_dataset = tf.data.Dataset.from_tensor_slices(\n",
        "    (\n",
        "        {\"input_1\": testdata},\n",
        "        {\"ed\": wtest, \"sd\": wbtest},\n",
        "    )\n",
