        "a = stan(a)\n",
        "w = stan(w)\n",
        "d = stan(d)\n",
        "m = stan(m)\n",
        "k = stan(k)\n",
        "t = stan(t)\n",
        "tim = stan(time)\n",
        "wb = pd.Series(wb)\n",
