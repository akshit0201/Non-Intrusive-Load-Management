      "source": [
        "def prepare(a,b):\n",
        "  temp = []\n",
        "  t = np.shape(a)[0]\n",
        "  for i in range(t):\n",
        "    temp.append(list(zip(a[i],b[i])))\n",
        "  return np.array(temp)\n"
      ],
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "o36hEB7aNEqq"
